@article{Desmet1999,
abstract = {We examined patterns of richness and endemism in relation to phylogeny (family membership), growth form and habitat in the flora of an arid ({\&}amp;lt60 mm annual rainfall), sandy coastal belt between Port Nolloth and Alexander Bay on the north-west coast of Namaqualand, South Africa. This region forms part of the species-rich Succulent Karoo biome. In an area of 750 km, 300 plant species were recorded with 24{\%} endemnicity. This level of diversity and endemism is exceptionally high for a desert region. Plant distribution is strongly influenced by habitat type. Diversity, especially of endemic species, was concentrated on rocky or unusual (lag-gravel pavements) habitats. Endemics were not random phylogenetic nor biological assemblages. Dwarf leaf-succulent shrubs, especially Mesembryanthemaceae, were significantly over-represented amongst the endemic component. The results are discussed in terms of a model for plant diversification in an arid environment with relatively predictable sources of moisture., The Plant Ecology of Namaqualand, South Africa},
author = {Desmet, P G and Cowling, R M},
journal = {Plant Ecology},
number = {1/2},
pages = {23--33},
publisher = {Springer},
title = {{Biodiversity, Habitat and Range-Size Aspects of a Flora from a Winter-Rainfall Desert in North-Western Namaqualand, South Africa}},
url = {http://www.jstor.org/stable/20050772 papers3://publication/uuid/8C7D2A28-2FD5-42AE-A3F1-32AF21BDB5A9},
volume = {142},
year = {1999}
}
@article{Jurgens1999,
abstract = {Populations of shrubs in a winter-rainfall (ca. 70 mm {\$}\backslashtext{\{}yr{\}}{\^{}}{\{}-1{\}}{\$} ) desert community (Succulent Karoo), dominated by leaf succulents, showed remarkable dynamics over a 17-yr period. After a severe drought in 1979, which caused high plant mortality, perennial species number in a permanent 10 m × 10 m plot doubled between 1980 and 1996, when the maximum of 41 species was recorded. Numbers of individuals of evergreen, leaf succulent shrubs also doubled over the same period, but showed fluctuations in response to dry years during the monitoring period. Detailed observations on the four dominant leaf succulent shrubs (all members of the Mesembryanthemaceae) between 1983 and 1996, showed species-specific patterns in population turnover. Mortality of all observed individuals ranged between 60{\%} and 85{\%}, and the proportion of the population that was recruited over this period ranged from 62{\%} to 89{\%}. In only one species did individuals persist throughout the monitoring period. Mean ages of individuals, excluding seedlings with lifespans of {\&}amp;lt1 yr, and individuals observed at the beginning of the monitoring period, ranged from 4.6 yr to 5.6 yr. Patterns of mortality and recruitment showed substantial differences among species and were not all attributable to rainfall patterns. Overall, the turnover of the shrub populations over the 15-yr monitoring period was remarkably high for a system of desert perennials. These unusual population patterns may explain the unique structure of leaf succulent-dominated communities in the Succulent Karoo., The Plant Ecology of Namaqualand, South Africa},
author = {J{\"{u}}rgens, N and Gotzmann, I H and Cowling, R M},
journal = {Plant Ecology},
number = {1/2},
pages = {87--96},
publisher = {Springer},
title = {{Remarkable Medium-Term Dynamics of Leaf Succulent Mesembryanthemaceae Shrubs in the Winter-Rainfall Desert of Northwestern Namaqualand, South Africa}},
url = {http://www.jstor.org/stable/20050776 papers3://publication/uuid/8574A685-FA3C-427E-AED8-463D96CDE083},
volume = {142},
year = {1999}
}
