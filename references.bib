@article{Byrne2009a,
abstract = {Global warming is causing ocean warming and acidification. The distribution of Heliocidaris erythrogramma coincides with the eastern Australia climate change hot spot, where disproportionate warming makes marine biota particularly vulnerable to climate change. In keeping with near-future climate change scenarios, we determined the interactive effects of warming and acidification on fertilization and development of this echinoid. Experimental treatments (20-26 degrees C, pH 7.6-8.2) were tested in all combinations for the 'business-as-usual' scenario, with 20 degrees C/pH 8.2 being ambient. Percentage of fertilization was high (>89{\%}) across all treatments. There was no difference in percentage of normal development in any pH treatment. In elevated temperature conditions, +4 degrees C reduced cleavage by 40 per cent and +6 degrees C by a further 20 per cent. Normal gastrulation fell below 4 per cent at +6 degrees C. At 26 degrees C, development was impaired. As the first study of interactive effects of temperature and pH on sea urchin development, we confirm the thermotolerance and pH resilience of fertilization and embryogenesis within predicted climate change scenarios, with negative effects at upper limits of ocean warming. Our findings place single stressor studies in context and emphasize the need for experiments that address ocean warming and acidification concurrently. Although ocean acidification research has focused on impaired calcification, embryos may not reach the skeletogenic stage in a warm ocean.},
author = {Byrne, Maria and Ho, Melanie and Selvakumaraswamy, Paulina and Nguyen, Hong D and Dworjanyn, Symon a and Davis, Andy R},
doi = {10.1098/rspb.2008.1935},
file = {:home/rws/Documents/Project/Articles/Byrne et al 2009 urchin climate change.pdf:pdf},
isbn = {0962-8452},
issn = {0962-8452},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {change,climate change,development,intergovernmental panel on climate,ocean acidification,ocean warming,scenarios,sea urchin fertilization and},
number = {1663},
pages = {1883--1888},
pmid = {19324767},
title = {{Temperature, but not pH, compromises sea urchin fertilization and early development under near-future climate change scenarios.}},
volume = {276},
year = {2009}
}
