pages = {214--222},
publisher = {Wiley Online Library},
title = {{Low Beta Diversity of Ambrosia Beetles (Coleoptera: Curculionidae: Scolytinae and Platypodinae) in Lowland Rainforests of Papua New Guinea}},
url = {papers3://publication/doi/10.2307/40235296?ref=no-x-route:bee606c52079aca6fa2c06152c96c1f3},
volume = {117},
