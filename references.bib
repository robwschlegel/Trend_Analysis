pages = {149--160},
publisher = {Springer},
title = {{A Preliminary Study of the Responsiveness to Seasonal Atmospheric and Rainfall Patterns of Wash Woodland Species in the Arid Richtersveld}},
url = {http://www.jstor.org/stable/20050781 papers3://publication/uuid/AAE880FA-EDC4-4DC6-895F-9947C5CA711E},
volume = {142},
year = {1999}
}
